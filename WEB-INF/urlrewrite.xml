<?xml version="1.0" encoding="utf-8"?>
    <!DOCTYPE urlrewrite
        PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN"
        "http://tuckey.org/res/dtds/urlrewrite3.0.dtd">

<urlrewrite>
	<rule match-type="regex">
		<from>^/invite/sns/([\w\-]+)$</from>
		<to>/usr/landing.jhtml?token=$1</to>
	</rule>
</urlrewrite>